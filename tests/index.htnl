<html>
  <head>
    <title>{{Data.meta.Meta.title}}</title>
  </head>
  <body>

    <h1>Sveikas {{ Data.user.User.name }}!</h1>
    {% if Data.user.User.role == admin %}
      <p>Adminitratorius</p>
    {% else %}
      <p>Dvarneska</p>
    {%endif%}

    <div class="ciklas">
      {% for Data.posts %}
        <h1>{{Post.title}}</h1>
      {% endfor %}
    </div>
  </body>
</html>



